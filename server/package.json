{
  "name": "account-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node build/app/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest \"test/unit/.*\\.spec\\.ts\"",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "integration": "jest \"test/integration/.*\\.integration\\.ts\"",
    "fix:bcrypt": "npm uninstall bcrypt && npm install bcrypt",
    "compose:dev": "docker-compose up --build",
    "compose:dev:kafka": "docker-compose -f docker/docker-compose-with-kafka.yml up --build",
    "compose:integration": "docker-compose -f docker/docker-compose.integration.yml up --build --abort-on-container-exit",
    "compose:dev:debug": "START_SCRIPT=start:dev:debug docker-compose up --build --abort-on-container-exit",
    "compose:dev:bash": "docker-compose exec account-service bash",
    "typeorm:schema": "node -r dotenv/config ./node_modules/typeorm/cli.js schema:log --config build/src/app/config/ormconfig.js",
    "typeorm:migrate": "node -r dotenv/config ./node_modules/typeorm/cli.js migration:run --config build/src/app/config/ormconfig.js",
    "typeorm:migrate:revert": "node -r dotenv/config ./node_modules/typeorm/cli.js migration:revert --config build/src/app/config/ormconfig.js",
    "typeorm:dev": "ts-node --files -r dotenv/config -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/app/config/ormconfig.ts",
    "typeorm:dev:schema": "ts-node -r dotenv/config -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:log --config src/app/config/ormconfig.ts",
    "typeorm:dev:migrate": "ts-node -r dotenv/config -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run --config src/app/config/ormconfig.ts",
    "typeorm:dev:migrate:revert": "ts-node -r dotenv/config -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert --config src/app/config/ormconfig.ts",
    "typeorm": "ts-node -r dotenv/config -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config build/src/app/config/ormconfig.js"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.4",
    "@nestjs/core": "^8.0.4",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^5.0.8",
    "@nestjs/typeorm": "^8.0.1",
    "aws-amplify": "^4.2.10",
    "axios": "^0.21.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^1.15.0",
    "lodash": "^4.17.21",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "uuid": "^8.3.2",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.18",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/kafkajs": "^1.9.0",
    "@types/lodash": "^4.14.171",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-jest": "^27.0.6",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "packageManager": "yarn@3.2.0",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$|.*\\.integration\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
